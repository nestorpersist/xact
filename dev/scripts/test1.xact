@ul {
   @x:for i := 1..3 {
      @x1:for j := 1..3 {
         @li { @x1:exp "[&i;,&j;]"++(i+j) }
      }
   }
}

@ul {
   @x:for j:=1..3 {
      @li { @x:exp "[1,&j;]"++(1+j) }
   }
   ...
}

@ul {
   @li { "[1,1]2" }
   ...
}

<ul>
   <li>[1,1]2</li>
   ...
</ul>


<ul>
   <@x:for i:= 1..10 { @b { i } }/>
</ul>

<ul>
   <@b { 1 }/>
   ...
</ul>
